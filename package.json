{
  "name": "baratie",
  "version": "0.0.1",
  "author": "dannycreations",
  "license": "GPL",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "format": "prettier --write .",
    "update": "npm-check-updates -u -x @types/node",
    "prepare": "husky && pnpm run build",
    "release": "pnpm run build && changeset publish --no-git-tag"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dannycreations/Baratie.git"
  },
  "homepage": "https://github.com/dannycreations/Baratie",
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "valibot": "^1.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^22.18.13",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "husky": "^9.1.7",
    "npm-check-updates": "^19.1.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vite-plugin-checker": "^0.11.0"
  },
  "bundleDependencies": [
    "@types/react",
    "@types/react-dom"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.20.0"
}
