    "valibot": "1.1.0",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "22.17.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "husky": "^9.1.7",
    "npm-check-updates": "^18.0.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
